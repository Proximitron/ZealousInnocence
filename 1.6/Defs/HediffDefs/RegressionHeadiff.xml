<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- START This was deprecated and is removed/replaced at game start -->
	<HediffDef>
		<defName>RegressionState</defName>
		<description>This person is mentally regressed. It will take some time for them to become their normal adult self. Until then, your colony has another baby.</description>
		<hediffClass>HediffWithComps</hediffClass>
		<label>regression</label>
		<everCurableByItem>false</everCurableByItem>
		<isBad>false</isBad>
		<initialSeverity>1</initialSeverity>
		<maxSeverity>1</maxSeverity>
		<taleOnVisible>RegressionRevealed</taleOnVisible>
		<comps>
			<li Class="HediffCompProperties_Disappears">
				<disappearsAfterTicks>6000000~6750000</disappearsAfterTicks>
			</li>
			<li Class="HediffCompProperties_SeverityPerDay">
				<severityPerDay>-0.019</severityPerDay>
			</li>
		</comps>
		<stages>
			<li>
				<label>teenager</label>
				<statFactors>
					<CertaintyLossFactor MayRequire="Ludeon.RimWorld.Ideology">1.2</CertaintyLossFactor>
				</statFactors>
				<minSeverity>0.01</minSeverity>
				<restFallFactor>1.25</restFallFactor>
				<hungerRateFactor>1.15</hungerRateFactor>
				<capMods>
					<li>
						<capacity>BladderControl</capacity>
						<offset>-0.2</offset>
					</li>
					<li>
						<capacity>Consciousness</capacity>
						<setMax>0.95</setMax>
					</li>
				</capMods>
				<hediffGivers>
					<li Class="HediffGiver_Random">
            <hediff>BladderInfection</hediff>
            <mtbDays>120</mtbDays>
            <partsToAffect>
              <li>Bladder</li>
            </partsToAffect>
          </li>
				</hediffGivers>
			</li>
			<li>
				<label>child</label>
				<statFactors>
					<CertaintyLossFactor MayRequire="Ludeon.RimWorld.Ideology">2</CertaintyLossFactor>
				</statFactors>
				<minSeverity>0.25</minSeverity>
				<restFallFactor>1.5</restFallFactor>
				<hungerRateFactor>1.5</hungerRateFactor>
				<vomitMtbDays>2.0</vomitMtbDays>
				<capMods>
					<li>
						<capacity>Manipulation</capacity>
						<offset>-0.35</offset>
					</li>
					<li>
						<capacity>Talking</capacity>
						<offset>-0.25</offset>
					</li>
					<li>
						<capacity>Moving</capacity>
						<offset>-0.25</offset>
					</li>
					<li>
						<capacity>Consciousness</capacity>
						<setMax>0.85</setMax>
					</li>
					<li>
            <capacity>BloodFiltration</capacity>
            <offset>-0.25</offset>
          </li>
					<li>
						<capacity>BladderControl</capacity>
						<offset>-0.5</offset>
					</li>
					<li>
            <capacity>Eating</capacity>
            <offset>-0.25</offset>
          </li>
				</capMods>
				<hediffGivers>
          <li Class="HediffGiver_Random">
            <hediff>Flu</hediff>
            <mtbDays>160</mtbDays>
            <partsToAffect>
              <li>Torso</li>
            </partsToAffect>
          </li>
					<li Class="HediffGiver_Random">
            <hediff>SleepingSickness</hediff>
            <mtbDays>160</mtbDays>
            <partsToAffect>
              <li>Torso</li>
            </partsToAffect>
          </li>
					<li Class="HediffGiver_Random">
            <hediff>BladderInfection</hediff>
            <mtbDays>160</mtbDays>
            <partsToAffect>
              <li>Bladder</li>
            </partsToAffect>
          </li>
        </hediffGivers>
				<forgetMemoryThoughtMtbDays>1</forgetMemoryThoughtMtbDays>
			</li>
			<li>
				<label>toddler</label>
				<statFactors>
					<CertaintyLossFactor MayRequire="Ludeon.RimWorld.Ideology">2.5</CertaintyLossFactor>
				</statFactors>
				<minSeverity>0.75</minSeverity>
				<vomitMtbDays>0.75</vomitMtbDays>
				<restFallFactor>2.0</restFallFactor>
				<hungerRateFactor>1.75</hungerRateFactor>
				<forgetMemoryThoughtMtbDays>5</forgetMemoryThoughtMtbDays>
				<capMods>
					<li>
						<capacity>Manipulation</capacity>
						<offset>-0.5</offset>
					</li>
					<li>
						<capacity>Talking</capacity>
						<offset>-0.5</offset>
					</li>
					<li>
						<capacity>Moving</capacity>
						<offset>-0.55</offset>
					</li>
					<li>
						<capacity>Consciousness</capacity>
						<setMax>0.8</setMax>
					</li>
					<li>
						<capacity>BladderControl</capacity>
						<offset>-0.95</offset>
					</li>
					<li>
            <capacity>BloodFiltration</capacity>
            <offset>-0.20</offset>
          </li>
					<li>
            <capacity>Eating</capacity>
            <offset>-0.5</offset>
          </li>
				</capMods>
				<hediffGivers>
          <li Class="HediffGiver_Random">
            <hediff>Flu</hediff>
            <mtbDays>100</mtbDays>
            <partsToAffect>
              <li>Torso</li>
            </partsToAffect>
          </li>
					<li Class="HediffGiver_Random">
            <hediff>SleepingSickness</hediff>
            <mtbDays>100</mtbDays>
            <partsToAffect>
              <li>Torso</li>
            </partsToAffect>
          </li>
					<li Class="HediffGiver_Random">
            <hediff>BladderInfection</hediff>
            <mtbDays>140</mtbDays>
            <partsToAffect>
              <li>Bladder</li>
            </partsToAffect>
          </li>
        </hediffGivers>
			</li>
			<li>
				<label>baby</label>
				<statFactors>
					<CertaintyLossFactor MayRequire="Ludeon.RimWorld.Ideology">3</CertaintyLossFactor>
				</statFactors>
				<minSeverity>0.9</minSeverity>
				<vomitMtbDays>0.5</vomitMtbDays>
				<restFallFactor>2.5</restFallFactor>
				<hungerRateFactor>2.5</hungerRateFactor>
				<capMods>
					<li>
						<capacity>Moving</capacity>
						<setMax>0</setMax>
					</li>
					<li>
						<capacity>Talking</capacity>
						<setMax>0</setMax>
					</li>
					<li>
						<capacity>Manipulation</capacity>
						<setMax>0</setMax>
					</li>
					<li>
						<capacity>BladderControl</capacity>
						<setMax>0</setMax>
					</li>
					<li>
						<capacity>Consciousness</capacity>
						<setMax>0.6</setMax>
					</li>
					<li>
            <capacity>BloodFiltration</capacity>
            <offset>-0.25</offset>
          </li>
					<li>
            <capacity>Eating</capacity>
            <offset>-0.75</offset>
          </li>
				</capMods>
				<hediffGivers>
          <li Class="HediffGiver_Random">
            <hediff>Flu</hediff>
            <mtbDays>50</mtbDays>
            <partsToAffect>
              <li>Torso</li>
            </partsToAffect>
          </li>
					<li Class="HediffGiver_Random">
            <hediff>SleepingSickness</hediff>
            <mtbDays>50</mtbDays>
            <partsToAffect>
              <li>Torso</li>
            </partsToAffect>
          </li>
        </hediffGivers>
			</li>
		</stages>
	</HediffDef>
	<!-- END This was deprecated and is removed/replaced at game start -->
	
	<HediffDef>
		<defName>RegressionDamageMental</defName>
		<description>This person is mentally regressed (made younger) as a result of received a dose of regressive nanites found only in the fountain of youth. This will make them act younger than they physically are.  While a full dose will heal the brain, an overdose is even able to revert death.</description>
		<hediffClass>ZealousInnocence.Hediff_RegressionDamageMental</hediffClass>
		<label>mental regression</label>
		<defaultLabelColor>(0.9, 1.0, 0.35)</defaultLabelColor>
		<everCurableByItem>false</everCurableByItem>
		<isBad>false</isBad>
		<taleOnVisible>RegressionRevealed</taleOnVisible>
		<comps>
		  <!-- <li Class="HediffCompProperties_SeverityPerDay">
        <severityPerDay>-0.1</severityPerDay>This controls how the severity changes over time, if at all 
      </li>-->
		</comps>
		<stages>
		  <li>
				<minSeverity>0.01</minSeverity>
      </li>
      <li>
				<minSeverity>1.201</minSeverity>
        <capMods>
					<li>
						<capacity>Moving</capacity>
						<setMax>0</setMax>
					</li>
					<li>
						<capacity>Talking</capacity>
						<setMax>0</setMax>
					</li>
					<li>
						<capacity>Manipulation</capacity>
						<setMax>0</setMax>
					</li>
					<li>
						<capacity>BladderControl</capacity>
						<setMax>0</setMax>
					</li>
        </capMods>
      </li>
    </stages>
		<initialSeverity>0.01</initialSeverity>
		<minSeverity>0</minSeverity>
		<maxSeverity>1.3</maxSeverity>
		<tendable>false</tendable>
	</HediffDef>
	
	<HediffDef>
		<defName>RegressionDamage</defName>
		<description>This person is physically regressed (made younger) as a result of received a dose of regressive nanites found only in the fountain of youth. Depending on severity this pawn will be affected in multiple ways, usually temporary. While a full dose will heal physical injuries, an overdose is even able to revert death</description>
		<hediffClass>ZealousInnocence.Hediff_RegressionDamage</hediffClass>
		<label>physical regression</label>
		<defaultLabelColor>(0.9, 1.0, 0.35)</defaultLabelColor>
		<everCurableByItem>false</everCurableByItem>
		<isBad>false</isBad>
		<taleOnVisible>RegressionRevealed</taleOnVisible>
		<comps>
		  <!-- <li Class="HediffCompProperties_SeverityPerDay">
        <severityPerDay>-0.1</severityPerDay>This controls how the severity changes over time, if at all 
      </li>-->
		</comps>
		<initialSeverity>0.01</initialSeverity>
		<minSeverity>0</minSeverity>
		<maxSeverity>1.3</maxSeverity>
		<tendable>false</tendable>
	</HediffDef>
</Defs>
