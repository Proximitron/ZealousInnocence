	<RecipeDef>
    <defName>Make_Apparel_Diaper_Disposable</defName>
    <label>make disposable diapers (10)</label>
    <description>Create a full package of simple, disposable, diapers.</description>
    <jobString>Making disposable diapers.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Tailor</effectWorking>
    <soundWorking>Recipe_Tailor</soundWorking>
    <workAmount>1200</workAmount>
    <allowMixingIngredients>true</allowMixingIngredients>
    <targetCountAdjustment>10</targetCountAdjustment>
    <recipeUsers>
      <li>ElectricTailoringBench</li>
      <li>HandTailoringBench</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Leathers</li>
						<li>Textiles</li>
          </categories>
          <disallowedThingDefs>

          </disallowedThingDefs>
        </filter>
        <count>50</count>
      </li>
    </ingredients>
		<fixedIngredientFilter>
      <categories>
        <li>Leathers</li>
				<li>Textiles</li>
      </categories>
      <disallowedThingDefs>

      </disallowedThingDefs>
    </fixedIngredientFilter>
		<defaultIngredientFilter>
			<categories>
				<li>Textiles</li>
				<li>Leathers</li>
			</categories>
			<disallowedThingDefs>
				<li>DevilstrandCloth</li>
				<li>Hyperweave</li>
				<li>Synthread</li>
				<li>Leather_Thrumbo</li>
				<li>Leather_Elephant</li>
				<li>Leather_Rhinoceros</li>
				<li>Leather_Bear</li>
				<li>Leather_Human</li>
				<li MayRequire="Ludeon.RimWorld.Odyssey">Leather_AlphaThrumbo</li>
			</disallowedThingDefs>
		</defaultIngredientFilter>
    <products>
      <Apparel_Diaper_Disposable>10</Apparel_Diaper_Disposable>
    </products>
    <workSkill>Crafting</workSkill>
    <displayPriority>0</displayPriority>
  </RecipeDef>
