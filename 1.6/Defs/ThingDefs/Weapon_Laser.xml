	<ThingDef ParentName="BaseHumanMakeableGun">
    <defName>ZI_FoyBlaster</defName>
    <label>squirt blaster</label>
    <description>A heavy weapon that can spray a stream of processed FOY water into an area and cause area wide regression. It can shoot in an arc over allies without harming them.\n\nWhile the main weapon can be used without limit, the weapon also comes equipped with a FOY-Vial-thrower to generate a massive blast of the water. Must be refueled after each use.</description>
    <relicChance>0</relicChance>
    <graphicData>
      <texPath>Things/Weapons/Water_Blaster</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Interact_Rifle</soundInteract>
    <generateCommonality>0.3</generateCommonality>
    <weaponClasses>
      <li>RangedHeavy</li>
    </weaponClasses>
    <statBases>
      <WorkToMake>30000</WorkToMake>
      <Mass>3.4</Mass>
      <RangedWeapon_Cooldown>3</RangedWeapon_Cooldown>
      <Flammability>0.7</Flammability>
    </statBases>
    <costList>
      <Steel>100</Steel>
			<ZI_Foy_Vial>10</ZI_Foy_Vial>
      <ComponentIndustrial>6</ComponentIndustrial>
    </costList>
    <recipeMaker>
      <skillRequirements>
        <Crafting>6</Crafting>
      </skillRequirements>
      <displayPriority>70</displayPriority>
    </recipeMaker>
    <thingSetMakerTags>
      <li>RewardStandardQualitySuper</li>
    </thingSetMakerTags>
    <verbs>
			<li>
				<verbClass>ZealousInnocence.Verb_ArcSprayWater</verbClass>
				<hasStandardCommand>true</hasStandardCommand>
				<warmupTime>0.5</warmupTime>
				<range>15.9</range>
				<minRange>3.9</minRange>
				<beamFullWidthRange>11</beamFullWidthRange>
				<burstShotCount>20</burstShotCount>
				<showBurstShotStats>false</showBurstShotStats>
				<requireLineOfSight>true</requireLineOfSight>
				<beamWidth>6</beamWidth>
				<ticksBetweenBurstShots>2</ticksBetweenBurstShots>

				<beamDamageDef>SquirtSpray</beamDamageDef>

				<soundCastTail>GunTail_Medium</soundCastTail>
				<muzzleFlashScale>9</muzzleFlashScale>

				<soundCastBeam>Flamethrower_Firing</soundCastBeam>

				<!-- visuals -->
				<beamGroundFleckDef>BlastExtinguisher</beamGroundFleckDef>
				<beamFleckChancePerTick>0.32</beamFleckChancePerTick>
				<beamMaxDeviation>0.5</beamMaxDeviation>
				<beamCurvature>0</beamCurvature>
				<beamEndEffecterDef>WaterBeam_End</beamEndEffecterDef>

				<!-- turn OFF any fire/ignition behavior -->
				<beamChanceToStartFire>0</beamChanceToStartFire>
				<beamFireSizeRange>0~0</beamFireSizeRange>
				<beamSetsGroundOnFire>false</beamSetsGroundOnFire>
				<ai_BeamIsIncendiary>true</ai_BeamIsIncendiary>

				<beamStartOffset>.5</beamStartOffset>
				<beamHitsNeighborCells>true</beamHitsNeighborCells>
				<beamCantHitWithinMinRange>true</beamCantHitWithinMinRange>
				<beamHitsNeighborCellsRequiresLOS>true</beamHitsNeighborCellsRequiresLOS>

				<beamLineFleckDef>Fleck_WaterBeamSmoke</beamLineFleckDef>

				<!-- cooler blue highlights -->
				<highlightColor>(40, 120, 220)</highlightColor>
				<secondaryHighlightColor>(180, 220, 255)</secondaryHighlightColor>

				<beamLineFleckChanceCurve>
					<points>
						<li>(0, 0)</li>
						<li>(0.65, 0.02)</li>
						<li>(1, .5)</li>
					</points>
				</beamLineFleckChanceCurve>

				<targetParams>
					<canTargetLocations>true</canTargetLocations>
				</targetParams>

				<!-- flammability curve no longer matters, but keep it harmless -->
				<flammabilityAttachFireChanceCurve>
					<points>
						<li>(0.6, 0)</li>
						<li>(0.7, 0)</li>
						<li>(1.2, 0)</li>
					</points>
				</flammabilityAttachFireChanceCurve>
			</li>
    </verbs>
